<?xml version="1.0" encoding="UTF-8"?>
<root>
  <rows>
    <rowdef name="BassRoot" desc="Bass Root" chord="C" />
    <rowdef name="BassMaj3" desc="Bass +M3" chord="E" />
    <rowdef name="BassMin3" desc="Bass +min3" chord="Eb"/>
    <rowdef name="ChordMajor" desc="Major Chord" shortname="M" chord="C E G"/>
    <rowdef name="ChordMinor" desc="Minor Chord" shortname="m" chord="C Eb G"/>
    <rowdef name="Chord7th" desc="7th Chord" shortname="7" chord="C E Bb"/>
    <rowdef name="ChordDim" desc="Dim Chord" shortname="Â°" chord="C Eb Bbb"/>
    <rowdef name="ChordAug" desc="Aug Chord" shortname="+" chord="C E G#"/>
  </rows>

  <boards>
<!--Notes Only)-->
    <board name="Notes Only" numColumns="12" filter="extra">
      <row name="BassMaj3"/>
      <row name="BassRoot"/>
    </board>
<!--8 Bass (4 x 2)-->
    <board numColumns="4">
      <row name="BassRoot"/>
      <row name="ChordMajor"/>
    </board>
<!--12 Bass (6 x 2)-->
